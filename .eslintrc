{
  "extends": "react-app",
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2017
  },

  "env": {
      "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "max-len": ["error", 120],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
